<!DOCTYPE html
  PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <link rel="icon" href="../../assets/img/icons/foundation-favicon.ico" type="image/x-icon">
  <meta name="viewport" content="width=device-width">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/foundation-emails/2.2.1/foundation-emails.min.css"
    rel="stylesheet">
  <title>Your Order</title>
</head>

<body>
  <span class="preheader"></span>



  <script>
    (function (i, s, o, g, r, a, m) {
    i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
      (i[r].q = i[r].q || []).push(arguments)
    }, i[r].l = 1 * new Date(); a = s.createElement(o),
      m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-2195009-2', 'auto');
    ga('send', 'pageview');

    ga('create', 'UA-2195009-27', 'auto', { name: "foundation" });
    ga('foundation.send', 'pageview');

    ga('create', 'UA-2195009-47', 'auto', { name: "foundation_emails" });
    ga('foundation_emails.send', 'pageview');

  </script>
  <table class="body">
    <tr>
      <td class="center" align="center" valign="top">
        <center data-parsed="">
          <style type="text/css" align="center" class="float-center">
            body,
            html,
            .body {
              background: #f3f3f3 !important;
            }
          </style>

          <table class="spacer float-center">
            <tbody>
              <tr>
                <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
              </tr>
            </tbody>
          </table>
          <table align="center" class="container float-center">
            <tbody>
              <tr>
                <td>
                  <table class="spacer">
                    <tbody>
                      <tr>
                        <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                      </tr>
                    </tbody>
                  </table>
                  <table class="row">
                    <tbody>
                      <tr>
                        <th class="small-12 large-12 columns first last">
                          <table>
                            <tr>
                              <th>
                                <h1>Thanks for your order.</h1>
                                <p>Thanks for shopping with us! We look forward to satisfying your recent order.</p>
                                <table class="spacer">
                                  <tbody>
                                    <tr>
                                      <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                                    </tr>
                                  </tbody>
                                </table>
                                <table class="callout">
                                  <tr>
                                    <th class="callout-inner secondary">
                                      <table class="row">
                                        <tbody>
                                          <tr>
                                            <th class="small-12 large-6 columns first">
                                              <table>
                                                <tr>
                                                  <th>
                                                    <p>
                                                      <strong>Payment Method</strong><br>
                                                      Stripe
                                                    </p>
                                                    <p>
                                                      <strong>Email Address</strong><br>
                                                      <a href="/cdn-cgi/l/email-protection" class="__cf_email__"
                                                        data-cfemail="83f7ebe6e0e2f3edc3f3eaf1e2f7e6f0adecf1e4"><%= @user.email %></a>
                                                    </p>
                                                    <p>
                                                      <strong>Order ID</strong><br>
                                                      <%= @order.id%>
                                                    </p>
                                                  </th>
                                                </tr>
                                              </table>
                                            </th>
                                            <th class="small-12 large-6 columns last">
                                              <table>
                                                <tr>
                                                  <th>
                                                    <p>
                                                      <strong>Shipping Address</strong><br>
                                                      <%= @user.first_name%> <%= @user.last_name%><br>
                                                      <%= @user.address%><br>
                                                      <%= @user.city%>
                                                      <%= @user.zip_code%>
                                                    </p>
                                                  </th>
                                                </tr>
                                              </table>
                                            </th>
                                          </tr>
                                        </tbody>
                                      </table>
                                    </th>
                                    <th class="expander"></th>
                                  </tr>
                                </table>
                                <h4>Order Details</h4>
                                <table>
                                  <% @order.items.each do |item| %>
                                  <tr>
                                    <td><%= item.title%></td>
                                    <td>1</td>
                                    <td><%= item.price%>€</td>
                                  </tr>
                                  <% end %>
                                  <tr>
                                    <th>Item</th>
                                    <th>#</th>
                                    <th>Price</th>
                                  </tr>
                                  <tr>
                                    <td colspan="2"><b>Subtotal:</b></td>
                                    <td><%= @order.total%>€</td>
                                  </tr>
                                </table>
                                <hr>
                                <h4>What's Next?</h4>
                                <p>Our carrier raven will prepare your order for delivery.</p>
                              </th>
                            </tr>
                          </table>
                        </th>
                      </tr>
                    </tbody>
                  </table>
                  <table class="row footer text-center">
                    <tbody>
                      <tr>
                        <th class="small-12 large-3 columns first">
                          <table>
                            <tr>
                              <th>
                                <img src="https://placehold.it/170x30" alt="">
                              </th>
                            </tr>
                          </table>
                        </th>
                        <th class="small-12 large-3 columns">
                          <table>
                            <tr>
                              <th>
                                <p>
                                  Call us at (+33) 6 12 34 56 78<br>
                                  Email us at admin4321@yopmail.com</a>
                                </p>
                              </th>
                            </tr>
                          </table>
                        </th>
                        <th class="small-12 large-3 columns last">
                          <table>
                            <tr>
                              <th>
                                <p>
                                  CLOTH-UP - Le Château<br>
                                  CAEN - 14 000 - FRANCE
                                </p>
                              </th>
                            </tr>
                          </table>
                        </th>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
            </tbody>
          </table>
        </center>
      </td>
    </tr>
  </table>

  <div style="display:none; white-space:nowrap; font:15px courier; line-height:0;"> &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; </div>
  <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
</body>

</html>